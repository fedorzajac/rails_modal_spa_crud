<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= post.content %>
  </p>

<table>

      <%#= link_to "Show this post", post %> 
      <tr>
        <td>
      <%= button_to "Edit this post", edit_post_path(post), method: :post%></td><td><%= button_to "Destroy this post", post, method: :delete %>
        </td>
      </tr>
</table>

</div>
